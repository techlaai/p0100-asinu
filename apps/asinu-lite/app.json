{
  "expo": {
    "name": "ASINU Lite",
    "slug": "asinu-lite",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "asinu-lite",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "newArchEnabled": false,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
